<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="au.org.nac.nactive">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
            android:name=".NACtiveApp"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".nactive.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".nactive.Setup">
        </activity>
        <activity android:name=".nactive.Information">
        </activity>
        <activity android:name=".nactive.ExercisesActivity">
        </activity>
        <activity android:name=".nactive.EditExercise">
        </activity>
        <activity android:name=".nactive.EditUser">
        </activity>
        <activity android:name=".nactive.SupportAct">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".nactive.EditWorkOut">
        </activity>
        <activity android:name=".nactive.UserDiary">
        </activity>
        <activity android:name=".nactive.Options">
        </activity>
    </application>

</manifest>